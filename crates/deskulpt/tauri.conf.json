{
  "productName": "Deskulpt",
  "version": "../../package.json",
  "identifier": "io.github.deskulptapps.deskulpt",
  "build": {
    "beforeDevCommand": "pnpm dev:js",
    "beforeBuildCommand": "pnpm build:js",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../../dist"
  },
  "app": {
    "macOSPrivateApi": true
  },
  "bundle": {
    "resources": {
      "../../resources": "resources"
    },
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
