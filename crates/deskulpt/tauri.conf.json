{
  "productName": "Deskulpt",
  "version": "../../package.json",
  "identifier": "com.deskulpt",
  "build": {
    "beforeDevCommand": "pnpm vite dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../../dist"
  },
  "app": {
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
